name:           afgame
version:        0.1
cabal-version:  >= 1.18
build-type:     Simple
author:         pierrer@pi3r.be

executable afgame
  default-language: Haskell2010
  hs-source-dirs:  src
  main-is:         Main.hs
  build-depends:  base >= 4,
                  mtl,
                  containers,
                  safe

  ghc-options:     -fno-warn-missing-signatures -O2 -ddump-rule-firings

